{
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "jsx": "preserve"
  }
}
