{
  "name": "guweb-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "dotenv -e .env.development nuxt dev",
    "generate": "nuxt generate",
    "preview:dev": "dotenv -e .env.development nuxt preview",
    "preview:prod": "nuxt preview",
    "start:prod": "dotenv -e .env node .output/server/index.mjs",
    "analyze": "nuxt analyze",
    "postinstall": "nuxt prepare",
    "lint:ts": "eslint --ext '.js,.vue,.ts,.jsx,.tsx' --ignore-path .gitignore .",
    "lint": "yarn lint:ts",
    "lintfix": "yarn lint:ts --fix",
    "lint:type-check": "tsc -p . --noEmit && vue-tsc --noEmit",
    "ts-node": "ts-node",
    "sync-from-lock": "yarn upgrade && npx syncyarnlock --save --keepPrefix && yarn install",
    "check-update": "npx npm-check-updates",
    "update-dep": "npx npm-check-updates -u && yarn install",
    "prisma:gen:dev": "dotenv -e .env.development yarn prisma generate -- ",
    "prisma:gen:prod": "yarn prisma generate"
  },
